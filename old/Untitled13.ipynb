    "def tens_to_mat(liste):\n",
    "    if isinstance(liste,list):\n",
    "        liste = np.array(liste)\n",
    "    res = np.array([[liste[0],liste[3],liste[4]],\n",
    "                    [liste[3],liste[1],liste[5]],\n",
    "                    [liste[4],liste[5],liste[2]]])\n",
    "    return res\n"
   ]
  },
  {
