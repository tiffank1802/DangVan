   },
   "outputs": [],
   "source": [
    "def contTang(tens,vN):\n",
    "    # calcul le vecteur contrainte tangentiell sur une facette de normale vN\n",
    "    M = tens_to_mat(tens)# vecteur contrainte\n",
    "    cont = M@vN# contrainte normale\n",
    " \n",
    "    cN = cont@vN\n",
    "    contT = cont-cN*vN\n",
    "    return contT\n"
   ]
  },
